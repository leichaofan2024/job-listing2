<!DOCTYPE html>
<html>
  <head>
    <title> admin:后台职缺列表 </title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
    <div class="container-fluid">
      <%= render "common/navbar" %>
  <%= render "common/flashes" %>
      <div class="row">

        <div class="col-md-2">
          <ul class="nav nav-pills nav-stacked" style="max-width: 150px; padding-top: 20px;">
            <li class="active"><%= link_to("后台列表", admin_jobs_path, :class =>"btn btn-default") %></li>
          </ul>
        </div>

        <br><br>
        <div class="col-md-10">

          <%= yield %>

        </div>
      </div>
      <div>
        <%= link_to("Return", jobs_path, :class => "btn btn-xs btn-success") %>
      </div>
    </div>
    <%= render "common/footer" %>
  </body>
</html>
